{
	"type": "module",
	"name": "v3-itsroseapi",
	"version": "1.0.0",
	"description": "ItsRose API",
	"main": "app.ts",
	"directories": {
		"test": "__tests__"
	},
	"scripts": {
		"test": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm __tests__/**/*.ts",
		"pretest": "npm run build && tsc -p __tests__/tsconfig.json",
		"start": "npm run build && fastify start -l info dist/app.js",
		"prebuild": "npm run clean",
		"build": "tsc",
		"watch": "tsc -w",
		"dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch\" \"npm:dev:start\"",
		"dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
		"lint": "eslint .",
		"format": "prettier --write src/**/*.ts",
		"clean": "rm -rf dist"
	},
	"keywords": [],
	"author": "FrierenDv",
	"license": "MIT",
	"dependencies": {
		"@fastify/autoload": "^6.1.0",
		"@fastify/cors": "^10.0.2",
		"@fastify/env": "^5.0.2",
		"@fastify/rate-limit": "^10.2.2",
		"@fastify/sensible": "^6.0.3",
		"@fastify/swagger": "^9.4.2",
		"@fastify/under-pressure": "^9.0.3",
		"fastify": "^5.2.1",
		"fastify-cli": "^7.3.0",
		"fastify-plugin": "^5.0.1",
		"fluent-json-schema": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.20.0",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/node": "^22.13.1",
		"c8": "^10.1.3",
		"concurrently": "^9.1.2",
		"eslint": "^9.20.0",
		"fastify-tsconfig": "^3.0.0",
		"globals": "^15.14.0",
		"prettier": "^3.5.0",
		"ts-node": "^10.9.2",
		"typescript": "~5.7.3",
		"typescript-eslint": "^8.23.0"
	}
}
